{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.dataxx.fr" }],
      "destination": "https://dataxx.fr/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}


