{
  "redirects": [
    { "source": "http://dataxx.fr/:path*", "destination": "https://dataxx.fr/:path*", "permanent": true },
    { "source": "https://www.dataxx.fr/:path*", "destination": "https://dataxx.fr/:path*", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}


